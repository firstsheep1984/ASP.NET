@model IEnumerable<OnlineAdmission.Models.EnclosedDocuments>

@{
    ViewBag.Title = "Index";
}

<h2>Enclosed Documents Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <td>
            @Html.DisplayNameFor(model => model.application.Id)
        </td>
       
        <th>
            @Html.DisplayNameFor(model => model.qualification)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.doctype)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.docname)
        </th>
 
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.applicationId)
    </td>
   
    <td>
        @Html.DisplayFor(modelItem => item.qualification)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.doctype)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.docname)
    </td>

    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
        @Html.ActionLink("Details", "Details", new { id = item.Id }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
    </td>
</tr>
}

</table>
